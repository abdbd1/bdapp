/* pages#home */
.jumbotron {
  background-image: asset-url('deskoffice.jpg');
  background-size: cover;
  height: 825px;
}

.jumbotron h1 {
  color: white !important;
  font-size: 110px !important;
}

.login {
  color: white;
  font-size: 33px;
  padding-top: 10px;
  margin-left: 73px;
}

.registro {
  color: white;
  font-size: 40px;
  margin-top: 140px;
  margin-left: 135px;
}

.login:hover, .registro .link-registro:hover {
  color: white;
  text-decoration: none;
}

.login:visited, .registro .link-registro:visited {
  color: white;
  text-decoration: none;
}

.quick-light {
  font-family: QuickSandLight;
}
